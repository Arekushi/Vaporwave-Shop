<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.arekushi.vaporwaveshop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_vaporwave"
        android:label="@string/app_name"
        android:logo="@mipmap/logo_vaporwave"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:theme="@style/AppTheme.Fullscreen" android:name=".activitys.RegisterActivity"></activity>
        <activity
            android:name=".activitys.LoginActivity"
            android:theme="@style/AppTheme.Fullscreen" />
        <activity
            android:name=".activitys.SplashActivity"
            android:theme="@style/AppTheme.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.MainActivity" />
    </application>

</manifest>